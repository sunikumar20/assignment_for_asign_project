= form_for [@project, @assign_project], remote: true do |f|
  = f.hidden_field :status, value: ASSIGNED
  .form-group
    %label{:for => "usr"} User Name:
    = f.autocomplete_field :user_id, autocomplete_user_email_users_path, class: "form-control"
  
  .form-group
    = f.submit "Submit", class: "btn btn-default"